<?xml version="1.0"?>
    <listadoInicial>
        <configInicial id="$codigoConfiguracion" idEmpresa="$codigoEmpresa" idPunto="$codigoPunto">
            <escritoriosActivos>
                <escritorio idEscritorio="$codigoEscritorio"/>
            </escritoriosActivos>
            <listadoClientes>
                <cliente dpi="$DPI">
                    <nombre> $nombreCliente </nombre>
                <listadoTransacciones>
                    <transaccion idTransaccion="$codTrans" cantidad="cantTrans"/>
                    <transaccion idTransaccion="$codTrans" cantidad="cantTrans"/>
                </listadoTransacciones>
                </cliente>
            </listadoClientes>
        </configInicial>
    </listadoInicial>